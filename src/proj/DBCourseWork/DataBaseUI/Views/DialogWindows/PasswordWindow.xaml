<Window x:Class="DataBaseUI.Views.DialogWindows.PasswordWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DataBaseUI.Views.DialogWindows"
        mc:Ignorable="d"
        Title="Ввод пароля" Height="100" Width="250">
    <StackPanel>
        <DockPanel Margin="3">
            <TextBlock Text="Введите пароль:" Margin="5"/>
            <TextBox x:Name="PasswordField" Margin="3"/>
        </DockPanel>
        <Button Content="Ок" Click="Button_Click" Margin="50, 5, 50, 5"/>
    </StackPanel>
</Window>
